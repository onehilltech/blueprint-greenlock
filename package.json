{
  "name": "@onehilltech/blueprint-greenlock",
  "description": "A blueprint module for Greenlock (TM).",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "test": "env NODE_ENV=test mocha tests/unit-tests --recursive",
    "cover": "env NODE_ENV=test istanbul cover _mocha tests/unit-tests --recursive",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "start-server": "env NODE_ENV=production forever -o ./app/.blueprint/logs/output.log -e ./app/.blueprint/logs/error.log start ./app/index.js",
    "stop-server": "env NODE_ENV=production forever stop app/index.js",
    "install-service": "sudo -s forever-service install blueprint-greenlock_d --script ./app/index.js",
    "uninstall-service": "sudo -s forever-service delete blueprint-greenlock_d"
  },
  "dependencies": {
    "@onehilltech/blueprint": "^4.5.4",
    "greenlock": "^2.4.2",
    "le-challenge-dns": "^2.3.2",
    "redirect-https": "^1.1.6"
  },
  "devDependencies": {
    "@onehilltech/blueprint-testing": "^0.8.0"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "keywords": [
    "blueprint",
    "blueprint-module",
    "greenlock",
    "le",
    "letsencrypt"
  ]
}
